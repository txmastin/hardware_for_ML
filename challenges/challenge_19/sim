#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib64/ivl/system.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib64/ivl/v2005_math.vpi";
:vpi_module "/usr/lib64/ivl/va_math.vpi";
S_0x563e46db2290 .scope module, "tb_binary_lif_neuron" "tb_binary_lif_neuron" 2 3;
 .timescale -9 -12;
v0x563e46dcf3d0_0 .var "clk", 0 0;
v0x563e46dcf490_0 .var "in", 0 0;
v0x563e46dcf560_0 .net "potential", 7 0, v0x563e46da2070_0;  1 drivers
v0x563e46dcf660_0 .var "rst", 0 0;
v0x563e46dcf730_0 .net "spike", 0 0, v0x563e46dcf220_0;  1 drivers
E_0x563e46d77080 .event posedge, v0x563e46d9f3c0_0;
S_0x563e46d76490 .scope module, "dut" "binary_lif_neuron" 2 16, 3 1 0, S_0x563e46db2290;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "in";
    .port_info 3 /OUTPUT 1 "spike";
    .port_info 4 /OUTPUT 8 "potential";
P_0x563e46d76670 .param/l "LEAK_DEN" 0 3 6, +C4<00000000000000000000000001100100>;
P_0x563e46d766b0 .param/l "LEAK_NUM" 0 3 5, +C4<00000000000000000000000001011010>;
P_0x563e46d766f0 .param/l "RESET_VAL" 0 3 4, +C4<00000000000000000000000000000000>;
P_0x563e46d76730 .param/l "THRESHOLD" 0 3 3, +C4<00000000000000000000000000000101>;
P_0x563e46d76770 .param/l "WIDTH" 0 3 2, +C4<00000000000000000000000000001000>;
v0x563e46d9f3c0_0 .net "clk", 0 0, v0x563e46dcf3d0_0;  1 drivers
v0x563e46da0fc0_0 .net "in", 0 0, v0x563e46dcf490_0;  1 drivers
v0x563e46da2070_0 .var "potential", 7 0;
v0x563e46da2850_0 .net "rst", 0 0, v0x563e46dcf660_0;  1 drivers
v0x563e46dcf220_0 .var "spike", 0 0;
E_0x563e46daf7b0 .event posedge, v0x563e46da2850_0, v0x563e46d9f3c0_0;
    .scope S_0x563e46d76490;
T_0 ;
    %wait E_0x563e46daf7b0;
    %load/vec4 v0x563e46da2850_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x563e46da2070_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x563e46dcf220_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x563e46da2070_0;
    %pad/u 32;
    %muli 90, 0, 32;
    %pushi/vec4 100, 0, 32;
    %div;
    %pad/u 8;
    %assign/vec4 v0x563e46da2070_0, 0;
    %load/vec4 v0x563e46da0fc0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x563e46da2070_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x563e46da2070_0, 0;
T_0.2 ;
    %load/vec4 v0x563e46da2070_0;
    %pad/u 32;
    %cmpi/u 5, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz  T_0.4, 5;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x563e46dcf220_0, 0;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x563e46da2070_0, 0;
    %jmp T_0.5;
T_0.4 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x563e46dcf220_0, 0;
T_0.5 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x563e46db2290;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563e46dcf3d0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x563e46dcf660_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x563e46db2290;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x563e46dcf3d0_0;
    %inv;
    %store/vec4 v0x563e46dcf3d0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x563e46db2290;
T_3 ;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563e46dcf660_0, 0, 1;
    %vpi_call 2 33 "$display", "\012--- Scenario 1: Constant input below threshold ---\012" {0 0 0};
    %vpi_call 2 34 "$display", "\012Time\011In\011  V\011Spike" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563e46dcf490_0, 0, 1;
    %pushi/vec4 10, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x563e46d77080;
    %vpi_call 2 38 "$display", "%g\011%b\011%d\011%b", $time, v0x563e46dcf490_0, v0x563e46dcf560_0, v0x563e46dcf730_0 {0 0 0};
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %vpi_call 2 44 "$display", "\012--- Scenario 2: Accumulating input until spike ---\012" {0 0 0};
    %vpi_call 2 45 "$display", "\012Time\011In\011  V\011Spike" {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x563e46dcf490_0, 0, 1;
    %pushi/vec4 6, 0, 32;
T_3.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.3, 5;
    %jmp/1 T_3.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x563e46d77080;
    %vpi_call 2 49 "$display", "%g\011%b\011%d\011%b", $time, v0x563e46dcf490_0, v0x563e46dcf560_0, v0x563e46dcf730_0 {0 0 0};
    %jmp T_3.2;
T_3.3 ;
    %pop/vec4 1;
    %vpi_call 2 55 "$display", "\012--- Scenario 3: Leakage with no input ---\012" {0 0 0};
    %vpi_call 2 56 "$display", "\012Time\011In\011  V\011Spike" {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x563e46dcf490_0, 0, 1;
    %pushi/vec4 3, 0, 32;
T_3.4 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.5, 5;
    %jmp/1 T_3.5, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x563e46d77080;
    %vpi_call 2 60 "$display", "%g\011%b\011%d\011%b", $time, v0x563e46dcf490_0, v0x563e46dcf560_0, v0x563e46dcf730_0 {0 0 0};
    %jmp T_3.4;
T_3.5 ;
    %pop/vec4 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563e46dcf490_0, 0, 1;
    %pushi/vec4 10, 0, 32;
T_3.6 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.7, 5;
    %jmp/1 T_3.7, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x563e46d77080;
    %vpi_call 2 66 "$display", "%g\011%b\011%d\011%b", $time, v0x563e46dcf490_0, v0x563e46dcf560_0, v0x563e46dcf730_0 {0 0 0};
    %jmp T_3.6;
T_3.7 ;
    %pop/vec4 1;
    %vpi_call 2 72 "$display", "\012--- Scenario 4: Strong input causing immediate spike ---\012" {0 0 0};
    %vpi_call 2 73 "$display", "\012Time\011In\011  V\011Spike" {0 0 0};
    %pushi/vec4 5, 0, 8;
    %force/vec4 v0x563e46da2070_0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x563e46dcf490_0, 0, 1;
    %wait E_0x563e46d77080;
    %vpi_call 2 77 "$display", "%g\011%b\011%d\011%b", $time, v0x563e46dcf490_0, v0x563e46dcf560_0, v0x563e46dcf730_0 {0 0 0};
    %release/reg v0x563e46da2070_0, 0, 8;
    %vpi_call 2 79 "$display", "\012" {0 0 0};
    %vpi_call 2 80 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
